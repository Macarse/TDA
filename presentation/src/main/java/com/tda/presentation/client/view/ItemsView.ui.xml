<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:style>
		.itemsViewButtonHPanel {
			margin: 5px 0px 0x 5px;
		}
		
		.itemsViewContactsFlexTable {
			margin: 5px 0px 5px 0px;
		}
	</ui:style>

	<g:DecoratorPanel>

		<g:VerticalPanel ui:field="itemsContainer">
			<g:HorizontalPanel addStyleNames="{style.itemsViewButtonHPanel}">
				<g:Button ui:field="addButton">Add</g:Button>
				<g:Button ui:field="deleteButton">Delete</g:Button>
			</g:HorizontalPanel>
		</g:VerticalPanel>

	</g:DecoratorPanel>
</ui:UiBinder> 