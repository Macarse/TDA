<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  
  <!-- Servlets -->
  <servlet>
    <servlet-name>greetServlet</servlet-name>
    <servlet-class>com.tda.server.GreetingServiceImpl</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>MedicFormServlet</servlet-name>
    <servlet-class>com.tda.server.MedicFormServiceImpl</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>greetServlet</servlet-name>
    <url-pattern>/tda/greet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>MedicFormServlet</servlet-name>
    <url-pattern>/tda/medic_form</url-pattern>
  </servlet-mapping>

  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>TDA.html</welcome-file>
  </welcome-file-list>

</web-app>
